<div class="row text-center">
  <h1>Dictionary</h1>
</div>

<div class="form-container">
  <form class="form-inline text-center" action="/word/new" method="post">
    <div class="form-group">
      <label for="name">Word to Define: </label>
      <input type="text" class="form-control custom-input" id="name" placeholder="Ex: 'word'">
    </div>

    <div class="form-group">
      <label for="origin">Origin of Word: </label>
      <input type="text" class="form-control custom-input" id="origin" placeholder="Ex: 'old english'">
    </div>
    <button type="submit" class="btn btn-primary">Add Word</button>
  </form>
</div>
